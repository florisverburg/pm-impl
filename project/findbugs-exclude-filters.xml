<FindBugsFilter>
  <!-- See docs/examples at http://findbugs.sourceforge.net/manual/filter.html -->
  <Match>
    <Class name="~views\.html\..*"/>
  </Match>
  <Match>
    <Class name="~Routes.*"/>
  </Match>
  <Match>
    <Class name="~controllers\.routes.*"/>
  </Match>
  <Match>
    <Class name="~models.*"/>
    <Field name="find" />
    <Bug pattern="MS_SHOULD_BE_FINAL" />
  </Match>
</FindBugsFilter>